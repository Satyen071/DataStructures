package ImplementLinkedList;

public class Test {

	public static void main(String[] args) {
		int[] arr = {198, 34, 3, 97, 9, 76, 45, 8};
		int[] result  = new int[20];
		int num ;
		int j=0;
		for(int i=0;i<arr.length-1;i++) {
			num = arr[i];
			
		while(num>0) {
			 int d = num%10;
			 num = num/10;
			 result[j] = d;
			 j++;
		}
		}
		
		for(int k=0;k<result.length-1;k++) {
			System.out.println(result[k]);
		}

	}

}
