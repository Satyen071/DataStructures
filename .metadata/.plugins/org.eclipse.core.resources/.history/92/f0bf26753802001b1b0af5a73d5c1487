package ImplementLinkedList;

public class TestString {

	public static void main(String[] args) {
		String s= "124,567,2345,124,6346,2";
		String[] s1 = s.split("");
		String[] s2 = new String[s1.length] ;
		 int[] temp = new int[s2.length];  
		int k=0,j=0,n=0;
		int num = s1.length;
		while(num>0) {
			if(s.charAt(k)==','||s.charAt(k)=='\0') {
				num--;
				k++;
				continue;
			}
			else {
				s2[j]=s1[k];
			j++;
			k++;
			}
			num--;
		}
		int number = 0;
		for(int i =0;i<s2.length-1;i++) {
			
				if(s2[i]!=s2[i+1]) {
					if(((int)s.charAt(i)>=48)&&((int)s.charAt(i)<=59))
		            {
						number = number*10+ ((int)s.charAt(i)-48);
						temp[n++] =number;
		            }	
				}
				
			  
			
		}
		for(int l=0;l<temp.length;l++) {
			System.out.print(temp[l]+" ");
		}
	}

}
