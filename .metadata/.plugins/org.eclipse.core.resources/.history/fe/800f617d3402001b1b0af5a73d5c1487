package ImplementLinkedList;

public class TestString {

	public static void main(String[] args) {
		String s= "124,567,2345,124,6346,2";
		String[] s1 = s.split("");
		int k=0,j=0;
		int num = s1.length;
		while(num>0) {
			if(s1[k]==",")
				num--;
			else {
				s1[j]=s1[k];
			j++;
			k++;
			}
		}
		for(int i =0;i<s1.length;i++) {
			System.out.println(s1[i]);
		}
	}

}
