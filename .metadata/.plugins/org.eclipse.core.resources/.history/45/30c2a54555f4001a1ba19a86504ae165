
public class TicTacToe {

	public static void main(String[] args) {
		
		char[][] gameBoard = {{' ','|',' ','|',' '},
				  			  {'-','+','-','+','-'},
				  			  {' ','|',' ','|',' '},
				  			  {'-','+','-','+','-'},
				  			  {' ','|',' ','|',' '}};
		
		showGameBoard(gameBoard);
		}

	public static void showGameBoard(char [][] gameBoard) {
		for(char[] row:gameBoard) {
			for(char c:row) {
				System.out.print(c);
			}
			System.out.println();
		}
	}
}
