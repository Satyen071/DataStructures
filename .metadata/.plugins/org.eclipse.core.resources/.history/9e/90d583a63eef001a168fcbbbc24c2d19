package StackandQueue;



class SimulQuestion
{
public static int sum(int n){
    int sum = 0; 
    while (n != 0)  
    { 
        sum = sum + n % 10; 
        n = n / 10; 
    } 
return sum;
}
  public static void main(String[] args)
  {
        //write your code here
int n = 10;
int i =1;
while(i!=0){
    if(i%n==0){
        
     if(sum(i)==n)
        System.out.println("output "+ i); 
    
    }
}




  }
}


